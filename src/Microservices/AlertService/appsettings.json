{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Grpc": "Debug"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Grpc": "Debug"
      }
    }
  },
  "OpenTelemetry": {
    "Endpoint": "http://localhost:4317"
  },
  "ConnectionStrings": {
    "AlertDb": "Data Source=alert-service.db"
  },
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    },
    "Endpoints": {
      "Grpc": {
        "Url": "http://localhost:5005",
        "Protocols": "Http2"
      },
      "Http": {
        "Url": "http://localhost:5015",
        "Protocols": "Http1"
      }
    }
  },
  "AllowedHosts": "*"
}
